{
    "name": "venveo/craft-boilerplate",
    "type": "project",
    "description": "Craft CMS Boilerplate for spinning up new projects.",
    "keywords": [
        "craft", "craftcms", "boilerplate", "craftboilerplate", "craft boilerplate"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/venveo/craft-boilerplate/issues",
        "source": "https://github.com/venveo/craft-boilerplate"
    },
    "require": {
        "php": "^7.2",
        "predis/predis": "1.1.1",
        "craftcms/cms": "^3.1.0",
        "vlucas/phpdotenv": "2.6.1",
        "craftcms/aws-s3": "^1.2.0",
        "craftcms/element-api": "^2.0.0",
        "craftcms/mailgun": "^1.4.0",
        "craftcms/redactor": "^2.3.0",
        "charliedev/element-map": "^1.2.0",
        "mmikkel/cp-field-inspect": "^1.0.0",
        "anubarak/craft-relabel": "^1.2.0",
        "sebastianlenz/linkfield": "^1.0.0",
        "nystudio107/craft-minify": "^1.2.0",
        "verbb/super-table": "^2.1.0",
        "verbb/field-manager": "^2.0.0",
        "yiisoft/yii2-redis": "2.0.9",
        "lukeyouell/craft-queue-manager": "1.1.0",
        "nystudio107/craft-seomatic": "^3.1.0",
        "nystudio107/craft-twigpack": "^1.1.0",
        "trendyminds/visor": "^2.1.0",
        "aelvan/imager": "^2.1.0",
        "nystudio107/craft-typogrify": "^1.1.0",
        "venveo/craft-redactor-split": "^1.0",
        "venveo/craft-redirect": "^2.0.0",
        "venveo/craft-bulkedit": "^1.0.0",
        "craftcms/feed-me": "4.0.0"
    },
    "suggest": {
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-imagick": "*"
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [],
        "craft:install": []
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
